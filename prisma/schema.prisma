// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./permits.db"
}


model clients {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  license       String
  country       String
  startDate       String
  endDate        String
  name      String
  status Boolean
}